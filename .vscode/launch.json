    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net10.0/V380Decoder.dll",
            "args": [
                "--id",
                "12345678",
                "--user",
                "admin",
                "--password",
                "password",
                "--ip",
                "192.168.1.2",
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}